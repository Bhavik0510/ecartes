<odoo>
    <data>
        <record id="ecartes_emd_manager_rule" model="ir.rule">
            <field name="name">Ecartes EMD Manager</field>
            <field name="model_id" ref="model_ecartes_emd"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field eval="[(4,ref('ecartes_emd.group_ecartes_emd_manager'))]" name="groups"/>
        </record>
        <record id="ecartes_emd_users_rule" model="ir.rule">
            <field name="name">Ecartes EMD User</field>
            <field name="model_id" ref="model_ecartes_emd"/>
            <field name="domain_force">[('create_uid.id', '=', user.id)]</field>
            <field eval="[(4,ref('ecartes_emd.group_ecartes_emd_user'))]" name="groups"/>

        </record>

    </data>
</odoo>