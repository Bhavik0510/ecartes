<odoo>
    <data>
        <record id="amc_expiry_cron" model="ir.cron">
            <field name="name">AMC Expire Cron</field>
            <field name="model_id" ref="model_amc_amc"/>
            <field name="state">code</field>
            <field name="code">model._check_amc_cron()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <!-- <field name="numbercall">-1</field> -->
            <field name="active" eval="True"/>
            <!-- <field name="doall" eval="False"/> -->
        </record>

        <record id="amc_visitor_crons" model="ir.cron">
            <field name="name">AMC visitor Cron</field>
            <field name="model_id" ref="model_amc_amc"/>
            <field name="state">code</field>
            <field name="code">model.get_amc_visitors()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <!-- <field name="numbercall">-1</field> -->
            <field name="active" eval="True"/>
            <!-- <field name="doall" eval="False"/> -->
        </record>

    </data>
</odoo>